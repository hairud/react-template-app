{
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "target": "ES5",
    "allowJs": false,
    "sourceMap": true,
    "module": "ESNext",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist", "config"]
}
